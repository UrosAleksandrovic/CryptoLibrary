<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MyCloudStore">

    <Color x:Key="BackgroundDark">#1b3f92</Color>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}"/>

    <Color x:Key="BackgroundLight">#95a9d6</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}"/>

    <Color x:Key="ComplementaryDarkColor">#926E1B</Color>
    <SolidColorBrush x:Key="ComplementaryDarkBrush" Color="{StaticResource ComplementaryDarkColor}"/>

    <Color x:Key="ComplementaryLightColor">#D6C295</Color>
    <SolidColorBrush x:Key="ComplementaryLightBrush" Color="{StaticResource ComplementaryLightColor}"/>

</ResourceDictionary>